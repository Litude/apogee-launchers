@ECHO OFF
SET SELECTION=1
:START
DARKAGES.EXE %SELECTION%
IF ERRORLEVEL 3 GOTO DA3
IF ERRORLEVEL 2 GOTO DA2
IF ERRORLEVEL 1 GOTO DA1
GOTO END

:DA3
SET SELECTION=3
SET EXENAME=DA3.EXE
GOTO RUN

:DA2
SET SELECTION=2
SET EXENAME=DA2.EXE
GOTO RUN

:DA1
SET SELECTION=1
SET EXENAME=DA1.EXE
GOTO RUN

:RUN
%EXENAME% %1 %2 %3 %4 %5 %6 %7 %8 %9
PAUSE > NUL
GOTO START

:END
SET SELECTION=
SET EXENAME=
