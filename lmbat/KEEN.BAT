@ECHO OFF
SET SELECTION=1
:START
KEEN.EXE %SELECTION%
IF ERRORLEVEL 3 GOTO KEEN3
IF ERRORLEVEL 2 GOTO KEEN2
IF ERRORLEVEL 1 GOTO KEEN1
GOTO END

:KEEN3
SET SELECTION=3
SET EXENAME=KEEN3.EXE
GOTO RUN

:KEEN2
SET SELECTION=2
SET EXENAME=KEEN2.EXE
GOTO RUN

:KEEN1
SET SELECTION=1
SET EXENAME=KEEN1.EXE
GOTO RUN

:RUN
%EXENAME% %1 %2 %3 %4 %5 %6 %7 %8 %9
PAUSE > NUL
GOTO START

:END
SET SELECTION=
SET EXENAME=
